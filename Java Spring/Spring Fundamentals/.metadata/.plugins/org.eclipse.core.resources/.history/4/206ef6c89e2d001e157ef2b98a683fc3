package com.codingdojo.mostrarlafecha;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
public class MostrarlafechaController {
	@RequestMapping("/")
	public String index() {
		return "index.jsp";
	}
	
	@RequestMapping("/date")
	public String fecha() {
		return "date.jsp";
	}
	
	@RequestMapping("/time")
	public String fecha() {
		return "time.jsp";
	}
}
